{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi-style": ["error", "last"],
        "semi": [2, "never"],
        "quotes": ["error", "single"],
        "no-unused-vars": "off",
        "react/prop-types": "off"
    }
}